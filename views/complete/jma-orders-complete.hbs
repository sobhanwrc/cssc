<div class="content">
    <div class="">
        <div class="page-header-title">
        </div>
    </div>
    <div class="page-content-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <h4 class="m-b-30 m-t-0 pull-left">Complete JMA Orders List</h4>
                            <div class="clearfix"></div>
                            <form class="form-inline m-b-30" role="form" name="filterorder" id="filterorder" method="POST" action="/jma-orders/complete" novalidate="">
                                <div class="form-group">
                                    <label class="sr-only" for="start_date">Start Date</label>
                                    <input type="text" class="form-control datepicker" name="start_date" id="start_date" value="{{start_date}}" placeholder="Enter start date">
                                </div>
                                <div class="form-group m-l-10">
                                    <label class="sr-only" for="end_date">End date</label>
                                    <input type="text" class="form-control datepicker" name="end_date" id="end_date" value="{{end_date}}"  placeholder="Enter end date">
                                </div>
                                <div class="form-group m-l-10">
                                    <label class="sr-only" for="part_no">Part No.</label>
                                    <!-- <input type="text" class="form-control" name="part_no" id="part_no" value="{{part_no}}" placeholder="Enter part no"> -->
                                    <select class="selectpicker form-control" name="part_no" data-live-search="true" title="All Components">
                                        <!-- <option value="all">All Components</option> -->
                                        {{#each part_nos}}
                                            <option value="{{this.pn}}" {{#if_eq this.pn ../part_no}}selected="selected"{{/if_eq}}>{{this.pn}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-success waves-effect waves-light m-l-10">Submit</button>
                                    <a href="javascript:history.back();"><button type="button" class="btn btn-success waves-effect waves-light m-l-10">Clear</button></a>
                                </div>
                            </form>
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    {{#if message}}
                                    <div class="alert alert-success">{{message}}</div>
                                    {{/if}}
                                    <table id="datatable" class="table table-striped table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Purchase Order</th>
                                                <th>Part No</th>
                                                <th>Location</th>
                                                <th>Price</th>
                                                <th>Order Date</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each orders}}
                                            <tr>
                                                <td>{{this.po}}</td>
                                                <td>{{this.pn}}</td>
                                                <td>{{this.location}}</td>
                                                <td>{{this.price}}</td>
                                                <td>{{dateFormat this.ordered "MM-DD-YYYY"}}</td>
                                                <td> <a title="Edit" href="/jma-orders/edit/{{this.id}}/"><button type="button" class="btn waves-effect btn-default edit-btn"> <i class="fa fa-edit"></i> </button></a></td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>